v0x13d7145c0_0 .net "a", 0 0, v0x13d714800_0;  1 drivers
v0x13d714660_0 .net "b", 0 0, v0x13d7148a0_0;  1 drivers
v0x13d714710_0 .net "y", 0 0, L_0x13d714b50;  alias, 1 drivers
    .scope S_0x13d704150;
T_0 ;
    %vpi_call 2 6 "$dumpfile", "nor_gate_tb.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars" {0 0 0};
    %vpi_call 2 8 "$monitor", "Time=%0t | a=%b | b=%b | y=%b", $time, v0x13d714800_0, v0x13d7148a0_0, v0x13d714950_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13d714800_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13d7148a0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13d714800_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x13d7148a0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x13d714800_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13d7148a0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x13d714800_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x13d7148a0_0, 0, 1;
    %delay 5, 0;
    %end;
    .thread T_0;
