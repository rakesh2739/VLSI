 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x13d714a20 .functor OR 1, v0x13d714800_0, v0x13d7148a0_0, C4<0>, C4<0>;
L_0x13d714b50 .functor NOT 1, L_0x13d714a20, C4<0>, C4<0>, C4<0>;
