S_0x13d704150 .scope module, "nor_gate_tb" "nor_gate_tb" 2 1;
 .timescale 0 0;
v0x13d714800_0 .var "a", 0 0;
v0x13d7148a0_0 .var "b", 0 0;
v0x13d714950_0 .net "y", 0 0, L_0x13d714b50;  1 drivers
