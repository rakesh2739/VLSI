S_0x12f7047b0 .scope module, "and_gate_tb" "and_gate_tb" 2 1;
 .timescale 0 0;
v0x12f714d50_0 .var "a", 0 0;
v0x12f714df0_0 .var "b", 0 0;
v0x12f714ea0_0 .net "y", 0 0, L_0x12f714f70;  1 drivers
S_0x12f704920 .scope module, "a1" "and_gate" 2 6, 3 1 0, S_0x12f7047b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x12f714f70 .functor AND 1, v0x12f714d50_0, v0x12f714df0_0, C4<1>, C4<1>;
v0x12f704b40_0 .net "a", 0 0, v0x12f714d50_0;  1 drivers
v0x12f714bb0_0 .net "b", 0 0, v0x12f714df0_0;  1 drivers
v0x12f714c50_0 .net "y", 0 0, L_0x12f714f70;  alias, 1 drivers
    .scope S_0x12f7047b0;
T_0 ;
    %vpi_call 2 8 "$dumpfile", "and_gate_tb.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars" {0 0 0};
    %vpi_call 2 10 "$monitor", "%0t a=%b b =%b y=%b", $time, v0x12f714d50_0, v0x12f714df0_0, v0x12f714ea0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12f714d50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12f714df0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12f714d50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12f714df0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12f714d50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12f714df0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12f714d50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12f714df0_0, 0, 1;
    %delay 5, 0;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
